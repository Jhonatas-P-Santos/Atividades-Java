import java.util.Scanner;

public class PrimeiroPrograma {

public static void main(String[] args) {


Scanner ler = new Scanner (System.in);
	
int n1,n2, prod; 

System.out.println("Escreva dois valores inteiro:");
 n1= ler.nextInt();
 n2= ler.nextInt();

prod= n1*n2;

   System.out.println("PROD = " + prod);

}
}


