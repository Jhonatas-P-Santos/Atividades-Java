import java.util.Scanner;

public class PrimeiroPrograma {

public static void main(String[] args) {


Scanner ler = new Scanner (System.in);

double n, raio , area;

n=3.14;

System.out.println("Escreva o valor de Raio:");
raio = ler.nextDouble();

area = n*(raio*raio);

System.out.println("Area: " + area);

}

}
