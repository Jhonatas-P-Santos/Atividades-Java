import java.util.Scanner;

public class CalculoSimples{
	public static void main(String[] args) {
		Scanner ler = new Scanner(System.in);

		int cod1, quant1, cod2, quant2;
		double valor1, valor2, total;

		System.out.println("Codigo da peca 1:");
		cod1 = ler.nextInt();

		System.out.println("Quantidade da peca 1:");
		quant1 = ler.nextInt();
	
		System.out.println("Valor da peca 1:");
		valor1 = ler.nextDouble();

		System.out.println("Codigo da peca 2:");
		cod2 = ler.nextInt();

		System.out.println("Quantidade da peca 2:");
		quant2 = ler.nextInt();

		System.out.println("Valor da peca 2:");
		valor2 = ler.nextDouble();

		total = (quant1 * valor1) + (quant2 * valor2);

		System.out.println("Total a pagar: " + total);


	}
}
